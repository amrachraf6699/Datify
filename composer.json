{
  "name": "amrachraf6699/datify",
  "description": "Automatic timestamp formatting helpers for Laravel created_at and updated_at columns.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
    "illuminate/database": "^8.0|^9.0|^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Datify\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Datify\\DatifyServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
